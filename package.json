{
  "name": "gotube-clone",
  "version": "1.0.0",
  "description": "clone youtube with vanillaJS and nodeJS",
  "main": "index.js",
  "dependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "autoprefixer": "^10.2.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.2.0",
    "heroku": "^7.49.0",
    "mini-css-extract-plugin": "^1.3.4",
    "mongoose": "^6.2.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "passport-oauth2": "^1.6.1",
    "postcss-loader": "^4.1.0",
    "pug": "^3.0.0",
    "sass-loader": "^10.1.1",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1"
  },
  "scripts": {
    "dev:db": "sudo mongod -f /etc/mongod.conf",
    "dev:assets": "cross-env WEBPACK_ENV=development webpack --watch",
    "dev:server": "nodemon --delay 2 --exec npx babel-node src/init.js",
    "build:assets": "cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build",
    "copyAll": "@powershell Copy-Item -Path src/static,src/views -Destination build -Recurse",
    "prebuild": "@powershell Remove-Item build -Recurse",
    "build": "npm run build:assets && npm run build:server && npm run copyAll",
    "prestart": "npm run build",
    "start": "cross-env PRODUCTION=true node build/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fantastiCake/gotube-clone.git"
  },
  "author": "fantasticCake",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fantastiCake/gotube-clone/issues"
  },
  "homepage": "https://github.com/fantastiCake/gotube-clone#readme"
}
